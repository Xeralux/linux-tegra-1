#include "jetson-platforms/tegra210-jetson-common-power-tree-p2530-0930-e03.dtsi"

/ {

	i2c@7000d000 {
		max77620@3c {
			backup-battery {
				maxim,backup-battery-charging-current = <800>;
				maxim,backup-battery-charging-voltage = <3500000>;
				maxim,backup-battery-output-resister = <100>;
			};

			pinmux@0 {
				pin_gpio1 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
				};

				pin_gpio2 {
					pins = "gpio2";
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
				};

				pin_gpio3 {
					pins = "gpio3";
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <4>;
					maxim,active-fps-power-down-slot = <3>;
				};
			};

			fps {
				fps0 {
					maxim,shutdown-fps-time-period-us = <1280>;
				};

				fps1 {
					maxim,shutdown-fps-time-period-us = <1280>;
				};
			};

			regulators {
				sd0 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <1>;
					maxim,active-fps-power-down-slot = <6>;
					regulator-ramp-delay = <27500>;
				};

				sd1 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <5>;
					maxim,active-fps-power-down-slot = <2>;
					regulator-ramp-delay = <27500>;
				};

				sd2 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <2>;
					maxim,active-fps-power-down-slot = <5>;
					regulator-ramp-delay = <27500>;
				};

				sd3 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <3>;
					maxim,active-fps-power-down-slot = <4>;
					regulator-ramp-delay = <27500>;
				};

				sd4 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-ramp-delay = <27500>;
				};

				ldo0 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-enable-ramp-delay = <84>;
					regulator-disable-ramp-delay = <14700>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <750>;
				};

				ldo1 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <3>;
					regulator-enable-ramp-delay = <34>;
					regulator-disable-ramp-delay = <660>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <350>;
				};

				ldo2 {
					regulator-name = "vddio-sdmmc1";
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-enable-ramp-delay = <300>;
					regulator-disable-ramp-delay = <64000>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <950>;
				};

				ldo3 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-ramp-delay = <100000>;
				};

				ldo4 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_0>;
					maxim,active-fps-power-up-slot = <1>;
					maxim,active-fps-power-down-slot = <6>;
					regulator-enable-ramp-delay = <23>;
					regulator-disable-ramp-delay = <1100>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <250>;
				};

				ldo5 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-ramp-delay = <100000>;
				};

				ldo6 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_NONE>;
					maxim,active-fps-power-up-slot = <0>;
					maxim,active-fps-power-down-slot = <7>;
					regulator-ramp-delay = <100000>;
				};

				ldo7 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <3>;
					maxim,active-fps-power-down-slot = <4>;
					regulator-enable-ramp-delay = <124>;
					regulator-disable-ramp-delay = <24000>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <1200>;
				};

				ldo8 {
					maxim,active-fps-source = <MAX77620_FPS_SRC_1>;
					maxim,active-fps-power-up-slot = <6>;
					maxim,active-fps-power-down-slot = <1>;
					regulator-enable-ramp-delay = <93>;
					regulator-disable-ramp-delay = <10000>;
					regulator-ramp-delay = <100000>;
					regulator-ramp-delay-scale = <900>;
				};

			};
		};
	};

	regulators {
		compatible = "simple-bus";
		device_type = "fixed-regulators";
		#address-cells = <1>;
		#size-cells = <0>;

		max77620_gpio7: regulator@2 {
			regulator-min-microvolt = <1050000>;
			regulator-max-microvolt = <1050000>;
			vin-supply = <&max77620_ldo7>;
		};

		en_vdd_sd: regulator@4 {
			gpio = <&gpio TEGRA_GPIO(Z, 3) GPIO_ACTIVE_HIGH>;
		};

		en_vdd_eth_5v0: regulator@207 {
			compatible = "regulator-fixed-sync";
			reg = <207>;
			regulator-name = "rtl-5v0";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio TEGRA_GPIO(H, 1) GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};

		en_usb_vbus2: regulator@208 {
			compatible = "regulator-fixed-sync";
			reg = <208>;
			regulator-name = "en-usb-vbus2";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio TEGRA_GPIO(CC, 5) GPIO_ACTIVE_HIGH>;
			gpio-open-drain;
			enable-active-high;
		};

		en_vdd_vcm_2v8: regulator@210 {
			compatible = "regulator-fixed-sync";
			reg = <210>;
			regulator-name = "en-vdd-vcm-2v8";
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <2800000>;
			vin-supply = <&vdd_3v3>;
			gpio = <&gpio TEGRA_GPIO(S, 4) 0>;
			enable-active-high;
		};

	};

	host1x {
		dc@54200000 {
			avdd_dsi_csi-supply = <&max77620_ldo0>;
			vdd_lcd_bl_en-supply = <&lcd_bl_en>;
			vdd_lcd_bl-supply = <&vdd_3v3>;

			/* eDP */
			avdd_io_edp-supply = <&max77620_gpio7>;
		};

		vi {
			avdd_dsi_csi-supply = <&max77620_ldo0>;
		};
	};

	pwm-fan {
		vdd-fan-supply = <&vdd_fan>;
	};

	otg@7d000000 {
		usb_vbus-supply = <&usb_vbus1>;
	};

	pinctrl@7009f000 {
		vbus-0-supply = <&usb_vbus1>;
		vbus-1-supply = <&en_vdd_eth_5v0>;
		vbus-2-supply = <&en_usb_vbus2>;
	};

	pmc-iopower {
		vddio-sys-supply = <&max77620_sd3>;
		vddio-uart-supply = <&max77620_sd3>;
		vddio-audio-supply = <&max77620_sd3>;
		vddio-cam-supply = <&max77620_sd3>;
		vddio-pex-ctrl-supply = <&max77620_sd3>;
		vddio-sdmmc1-supply = <&max77620_ldo2>;
		vddio-sdmmc3-supply = <&max77620_sd3>;
		vddio-sdmmc4-supply = <&max77620_sd3>;
		vddio-audio-hv-supply = <&max77620_sd3>;
		vddio-debug-supply = <&max77620_sd3>;
		vddio-dbg-supply = <&max77620_sd3>;
		vddio-dmic-supply = <&max77620_sd3>;
		vddio-gpio-supply = <&max77620_sd3>;
		vddio-spi-supply = <&max77620_sd3>;
		vddio-spi-hv-supply = <&max77620_sd3>;
		vddio-sdmmc2-supply = <&max77620_sd3>;
		vddio-dp-supply = <&max77620_sd3>;
	};

	pmc@7000e400 {
		io-pad-defaults {
			audio-hv {
				nvidia,io-pad-init-voltage = <IO_PAD_VOLTAGE_1_8V>;
			};

			spi-hv {
				nvidia,io-pad-init-voltage = <IO_PAD_VOLTAGE_1_8V>;
			};

			gpio {
				nvidia,io-pad-init-voltage = <IO_PAD_VOLTAGE_1_8V>;
			};
			sdmmc3 {
				nvidia,io-pad-init-voltage = <IO_PAD_VOLTAGE_1_8V>;
			};
		};
	};
};
